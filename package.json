{
  "name": "shringarcss-intellisense",
  "displayName": "Shringar CSS",
  "description": "A Visual Studio Code extension for providing intellisense for global Shringar CSS classes.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "author": {
    "name": "Abhay Sharma",
    "email": "abhay.sharma@smallcase.com"
  },
  "publisher": "smallcase",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smallcase/sc-fe-shringarcss-intellisense.git"
  },
  "icon": "smallcase-icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "css",
        "extensions": [
          ".css"
        ],
        "aliases": [
          "CSS"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.custom-css-intellisense",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/css": "^0.0.33",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.3.5",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "css": "^3.0.0",
    "postcss": "^8.0.0",
    "postcss-import": "^15.1.0"
  }
}
